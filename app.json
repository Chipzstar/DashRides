{
  "name": "DashRides",
  "displayName": "DashRides",
  "expo": {
    "name": "DashRides",
    "slug": "DashRides",
    "version": "1.0.0",
    "android": {
      "googleServicesFile": "./android/app/google-services.json"
    },
    "assetBundlePatterns": [
      "./app/assets/images/*",
      "./app/assets/icons/*",
      "./app/assets/splash/*"
    ],
    "icon": "./app/assets/splash/logo.png",
    "splash": {
      "image": "./app/assets/splash/splash.png",
      "resizeMode": "cover"
    },
    "androidStatusBar": {
      "translucent": true,
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    }
  }
}
